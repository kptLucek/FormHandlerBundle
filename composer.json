{
    "name": "lucek/form-handler-bundle",
    "type": "symfony-bundle",
    "description": "Bundle provides some logic for form handling process.",
    "keywords": [
        "form",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Bonk",
            "email": "marcin@lucek.io"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/dependency-injection": "~3.4||~4.4",
        "symfony/config": "~3.4||~4.4",
        "symfony/form": "~3.4||~4.4",
        "symfony/http-kernel": "~3.4||~4.4",
        "symfony/validator": "~3.4||~4.4",
        "symfony/event-dispatcher": "~3.4||~4.4"
    },

    "autoload": {
        "psr-4": {
            "Lucek\\FormHandlerBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "g1a/composer-test-scenarios": "^3.2"
    },
    "extra": {
        "scenarios": {
            "symfony3_71": {
                "require": {
                    "symfony/dependency-injection": "~3.4",
                    "symfony/config": "~3.4",
                    "symfony/form": "~3.4",
                    "symfony/http-kernel": "~3.4",
                    "symfony/validator": "~3.4",
                    "symfony/event-dispatcher": "~3.4"
                },
                "config": {
                    "platform": {
                        "php": "7.1.3"
                    }
                }
            },
            "symfony3_72": {
                "require": {
                    "symfony/dependency-injection": "~3.4",
                    "symfony/config": "~3.4",
                    "symfony/form": "~3.4",
                    "symfony/http-kernel": "~3.4",
                    "symfony/validator": "~3.4",
                    "symfony/event-dispatcher": "~3.4"
                },
                "config": {
                    "platform": {
                        "php": "7.2.34"
                    }
                }
            },
            "symfony3_73": {
                "require": {
                    "symfony/dependency-injection": "~3.4",
                    "symfony/config": "~3.4",
                    "symfony/form": "~3.4",
                    "symfony/http-kernel": "~3.4",
                    "symfony/validator": "~3.4",
                    "symfony/event-dispatcher": "~3.4"
                },
                "config": {
                    "platform": {
                        "php": "7.3.23"
                    }
                }
            },
            "symfony4_71": {
                "require": {
                    "symfony/dependency-injection": "~4.4",
                    "symfony/config": "~4.4",
                    "symfony/form": "~4.4",
                    "symfony/http-kernel": "~4.4",
                    "symfony/validator": "~4.4",
                    "symfony/event-dispatcher": "~4.4"
                },
                "config": {
                    "platform": {
                        "php": "7.1.3"
                    }
                }
            },
            "symfony4_72": {
                "require": {
                    "symfony/dependency-injection": "~4.4",
                    "symfony/config": "~4.4",
                    "symfony/form": "~4.4",
                    "symfony/http-kernel": "~4.4",
                    "symfony/validator": "~4.4",
                    "symfony/event-dispatcher": "~4.4"
                },
                "config": {
                    "platform": {
                        "php": "7.2.34"
                    }
                }
            },
            "symfony4_73": {
                "require": {
                    "symfony/dependency-injection": "~4.4",
                    "symfony/config": "~4.4",
                    "symfony/form": "~4.4",
                    "symfony/http-kernel": "~4.4",
                    "symfony/validator": "~4.4",
                    "symfony/event-dispatcher": "~4.4"
                },
                "config": {
                    "platform": {
                        "php": "7.3.23"
                    }
                }
            }
        }
    }
}
